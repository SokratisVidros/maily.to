{
  "name": "@maily-to/render",
  "version": "0.0.9",
  "private": false,
  "description": "A transformer that converts Maily content into HTML email templates.",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "browser": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./jsx": {
      "browser": "./dist/jsx/index.mjs",
      "import": "./dist/jsx/index.mjs",
      "require": "./dist/jsx/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "jsx": [
        "dist/jsx/index.d.ts"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:watch": "vitest",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arikchakma/maily.to.git",
    "directory": "packages/render"
  },
  "author": "Arik Chakma <arikchangma@gmail.com>",
  "keywords": [
    "maily.to",
    "react",
    "email"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.7",
    "@tiptap/core": "^2.4.0",
    "@types/lodash": "^4.17.5",
    "@types/react": "^18.3.3",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@react-email/components": "^0.0.19",
    "@react-email/render": "^0.0.15",
    "jsx-email": "^1.12.1",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "react": "18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
